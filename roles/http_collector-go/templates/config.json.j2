		"Addr": ":8091",
		"ReadTimeout": 5000000000,
		"WriteTimeout": 10000000000,
		"IdleTimeout": 60000000000
	},
	"AggTopic": "{{AggTopic}}",
	"WebTLS": {
		"CertFile": "",
		"KeyFile": "",
		"CaFile": ""
	},
